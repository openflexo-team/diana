<?xml version="1.0" encoding="ISO-8859-1"?>

<project name="Maven" xmlns="http://maven.apache.org/DECORATION/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">

	<bannerLeft>
		<name>Diana</name>
		<src>${diana.logo}</src>
		<href>https://openflexo.org/tiki/Diana</href>
	</bannerLeft>
	<bannerRight>
		<name>Openflexo</name>
		<src>${organization.logo}</src>
		<href>http://openflexo.org/</href>
	</bannerRight>
	<skin>
		<groupId>org.openflexo</groupId>
		<artifactId>openflexo-skin</artifactId>
		<version>1.3-SNAPSHOT</version>
	</skin>

	<custom>
		<openflexo-skin>
			<sideMenus>true</sideMenus>
			<displayBreadCrumbs>true</displayBreadCrumbs>
			<localMenu>/inc/html/developersMenu.html</localMenu>
			<includeVersionLinks>true</includeVersionLinks>
		</openflexo-skin>
	</custom>

	<body>

		<menu name=""
			img="https://support.openflexo.org/images/components/diana/diana_logo2.png">
		</menu>

		<menu name="Diana">
			<item name="Home" href="index.html" />
			<item name="Diana Geom" href="diana-geom/index.html" />
			<item name="Diana API" href="diana-api/index.html" />
			<item name="Diana Core" href="diana-core/index.html" />
			<item name="Diana Swing" href="diana-swing/index.html" />
			<item name="Drawing Editor" href="diana-drawing-editor/index.html" />
			<item name="GeomEdit" href="diana-geomedit/index.html" />
			<item name="FAQ" href="faq.html" />
			<item name="Javadoc" href="./apidocs/index.html" />
		</menu>

		<menu name="HowTos">
			<item name="Getting started with Diana" href="GettingStartedWithDiana.html" />
			<item name="Define a new layout manager" href="HowToDefineALayoutManagerInDianaEngine.html" />
		</menu>

		<menu ref="modules" />
		<menu ref="reports" />
	</body>

</project>
